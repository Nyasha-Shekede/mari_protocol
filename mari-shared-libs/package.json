{
  "name": "@mari/shared-libs",
  "version": "1.0.0",
  "description": "Shared libraries and utilities for the Mari protocol ecosystem",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c --bundleConfigAsCjs",
    "dev": "rollup -c -w --bundleConfigAsCjs",
    "test": "jest",
    "lint": "eslint src/",
    "docs": "typedoc --options typedoc.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build && npm run test"
  },
  "keywords": [
    "mari",
    "protocol",
    "physics",
    "crypto",
    "utilities"
  ],
  "author": "Mari Team",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.19.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.5.0",
    "rollup": "^3.20.0",
    "rollup-plugin-dts": "^6.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.5.0",
    "typedoc": "^0.25.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "base64-js": "^1.5.1",
    "js-sha256": "^0.9.0"
  }
}
